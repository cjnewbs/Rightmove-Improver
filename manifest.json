{
  "manifest_version": 2,

  "name": "Rightmove Improver",
  "description": "This extension adds maximum internet speed to the page on rightmove",
  "version": "0.1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "About"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.rightmove.co.uk/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}